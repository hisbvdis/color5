{
  "$schema": "vscode://schemas/color-theme",
  "type": "light",
  "name": "Color5",
  "colors": {
    "activityBarBadge.background": "#007acc",
    "editor.background": "#fafafa",
    "editor.foreground": "#000000",
    "editor.inactiveSelectionBackground": "#e5ebf1",
    "editor.lineHighlightBorder": "#00000000",
    "editor.rangeHighlightBackground": "#00000000",
    "editor.selectionHighlightBackground": "#add6ff80",
    "editorGroupHeader.tabsBackground": "#d4d4d4",
    "editorIndentGuide.activeBackground": "#b3b3b3",
    "editorIndentGuide.background": "#dedede",
    "editorSuggestWidget.background": "#f3f3f3",
    "input.placeholderForeground": "#767676",
    "list.focusHighlightForeground": "#9dddff",
    "list.hoverBackground": "#e8e8e8",
    "notebook.cellBorderColor": "#e8e8e8",
    "notebook.selectedCellBackground": "#c8ddf150",
    "ports.iconRunningProcessForeground": "#369432",
    "searchEditor.textInputBorder": "#cecece",
    "settings.numberInputBorder": "#cecece",
    "settings.textInputBorder": "#cecece",
    "sideBar.border": "#dbdbdc",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#61616130",
    "sideBarTitle.foreground": "#6f6f6f",
    "statusBarItem.errorBackground": "#c72e0f",
    "statusBarItem.remoteBackground": "#16825d",
    "statusBarItem.remoteForeground": "#ffffff",
    "tab.activeBackground": "#fafafa",
    "tab.border": "#cccccc",
    "tab.inactiveBackground": "#e3e3e3",
    "tab.lastPinnedBorder": "#61616130",
    "list.activeSelectionIconForeground": "#ffffff",
  },
  "tokenColors": [{
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key string",
        "keyword.other.unit",

        "meta.embedded",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "storage",
        "keyword",
      ],
      "settings": {
        // "foreground": "#1500ff"
        "foreground": "#2a00ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
      ],
      "settings": {
        // "foreground": "#0099cc"
        "foreground": "#00a1e6"
      }
    },
    {
      "scope": [
        "string",
      ],
      "settings": {
        "foreground": "#009900"
      }
    },
    {
      "scope": [
        
      ],
      "settings": {
        "foreground": "#c26e00"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
      ],
      "settings": {
        "foreground": "#8250DF"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#996bc7"
      }
    },
    {
      "scope": [
        "entity.name.tag",
      ],
      "settings": {
        "foreground": "#CF222E"
      }
    },
    {
      "scope": [
        "markup.heading",
      ],
      "settings": {
        "foreground": "#a21319"
      }
    },
    {
      "scope": [
        "comment",
      ],
      "settings": {
        "foreground": "#969896"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.bold",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "scope": [
        
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
  ],
}